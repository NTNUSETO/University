a,b= eval(input('請輸入 年/月/日 並用逗號隔開:'))
if b<1 or b>12 :
  print('你在鬧啊')
else:
  if a%400 == 0:
    year = '潤'
    print('這年是',year,'年')
  elif a%4 == 0 and a%100 != 0:
    year = '潤'
    print('這年是',year,'年')
  else:
    year = '平'
    print('這年是',year,'年')
  if b == 1 or 3 or 5 or 7 or 8 or 10 or 12:
    print('這月有 31 天')
  elif b == 4 or 6 or 9 or 11:
    print('這月有 30 天')
  elif b == 2 and year == '潤':
    print('這月有 29 天')
  else:
    print('這月有 28 天')
